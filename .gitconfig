[user]
	name = davroid
	email = 88601542+davroid@users.noreply.github.com
[init]
	defaultBranch = main
[pull]
	rebase = true
	autoSetupRemote = true
[merge]
	conflictStyle = zdiff3
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
  dark = true
  line-numbers = true
  side-by-side = true
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[push]
	autoSetupRemote = true
